{
  "manifest_version": 3,
  "name": "Unigraph",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "scripting",
    "notifications",
    "identity",
    "storage"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "host_permissions": ["*://*/*"],
  "oauth2": {
    "client_id": "1089489360036-8gmtvjgpd0p8dptqfdrhaj0cfd6glvp5.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.PNG",
      "32": "icons/icon16.PNG"
    }
  },
  "icons": {
    "16": "icons/icon16.PNG",
    "48": "icons/icon16.PNG",
    "128": "icons/icon16.PNG"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["area-capture.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "key": ""
}
